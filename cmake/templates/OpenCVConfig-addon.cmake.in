set(OpenCV_EXTRA_MODULES @OPENCV_MODULES_CONFIGCMAKE@)

set(OpenCV_LIB_COMPONENTS ${OpenCV_LIB_COMPONENTS} ${OpenCV_EXTRA_MODULES})
set(OpenCV_INCLUDE_DIRS ${OpenCV_INCLUDE_DIRS} @OpenCV_INCLUDE_DIRS_CONFIGCMAKE@)

list(GET OpenCV_EXTRA_MODULES 0 __module)
if(NOT TARGET ${__module})
  include(${CMAKE_CURRENT_LIST_DIR}/../@OpenCV_PACKAGE_NAME@Modules.cmake)
endif()
