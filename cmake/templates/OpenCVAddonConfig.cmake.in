set(OpenCV_SKIP_ADDONS ${OpenCV_SKIP_ADDONS} @OpenCV_PACKAGE_NAME@)
find_package(OpenCV @OpenCV_VERSION@ EXACT REQUIRED)

set(OpenCV_EXTRA_MODULES @OPENCV_MODULES_CONFIGCMAKE@)

list(GET OpenCV_EXTRA_MODULES 0 __module)
if(NOT TARGET ${__module})
  include("${CMAKE_CURRENT_LIST_DIR}/@OpenCV_BASECONFIG_PATH_RELATIVE@/@OpenCV_PACKAGE_NAME@Modules.cmake")
endif()

set(OpenCV_LIB_COMPONENTS ${OpenCV_LIB_COMPONENTS} ${OpenCV_EXTRA_MODULES})
set(OpenCV_LIBS ${OpenCV_LIBS} ${OpenCV_EXTRA_MODULES})
set(OpenCV_INCLUDE_DIRS ${OpenCV_INCLUDE_DIRS} @OpenCV_INCLUDE_DIRS_CONFIGCMAKE@)

if(NOT @OpenCV_PACKAGE_NAME@_FIND_QUIETLY)
  message(STATUS "@OpenCV_PACKAGE_NAME@ modules: ${OpenCV_EXTRA_MODULES}")
endif()
